{
    "SublimeLinter.linters.flake8.executable":
    [
        "~/.pyenv/shims/python",
        "-m",
        "flake8"
    ],
    "SublimeLinter.linters.flake8.max-line-length": 120,
    "SublimeLinter.linters.yamllint.executable":
    [
        "~/.pyenv/shims/python",
        "-m",
        "yamllint"
    ],
    "always_show_minimap_viewport": true,
    "auto_format_on_save": true,
    "bold_folder_labels": true,
    "color_scheme": "Packages/Theme - Dark Material/schemes/Dark-Material.tmTheme",
    "detect_indentation": false,
    "font_face": "Ubuntu Mono derivative Powerline",
    "font_options":
    [
        "gray_antialias"
    ],
    "font_size": 12,
    "ignored_packages":
    [
        "Markdown",
        "RestructuredText",
        "Vintage"
    ],
    "line_padding_bottom": 3,
    "line_padding_top": 3,
    "node_path": "/opt/homebrew/bin/node",
    "open_files_in_new_window": false,
    "overlay_scroll_bars": "enabled",
    "paths":
    {
        "linux":
        [
        ],
        "osx":
        [
            "~/.pyenv/shims"
        ],
        "windows":
        [
        ]
    },
    "prettier_cli_path": "/opt/homebrew/bin/prettier",
    "show_defitions": false,
    "theme": "Dark-Material.sublime-theme",
    "translate_tabs_to_spaces": true,
    "folder_exclude_patterns": [".svn", ".git", ".hg", "CVS", "*.venv", "*.idea", "*.tox", "*.egg-info"]
}
